        <h2 class="about-page-title text-center pb-30">Jadwal Program</h2>
        <div class="row justify-content-center">
          <div class="col-md-4">
            <!-- single pricing plan -->
            <div class="single--pricing-plan active waves-effect">
              <!--plan top -->
              <div class="pricing-plan-top">
                <div class="plan-top-head">
