    <!-- contact section -->
    <section class="pt-130 pb-130" data-animate="fadeInUp" data-delay="1.05">
      <div class="container">
        <div class="row align-items-center">
          <div class="col-md-6">
            <!-- contact info -->
            <div class="contact-form-info">
              <h2 class="about-page-title">Keep In Touch</h2>
              <p>
                We want to hear from you. Out team is happy to answer your
                question. Fill out the form and we'll be in touch as soon as
                posible.
              </p>
            </div>
            <!-- End of contact info -->
            <div class="row">
              <div class="col-md-10">
                <!-- contact form -->
                <div class="contact-form contact-page-form parsley-validate">
                  <form action="" method="post" novalidate>
                    <!-- form group -->
                    <div class="form-group">
                      <input
                        type="text"
                        name="name"
                        class="theme-input-style"
                        placeholder="Full Name"
                        required
                      />
                    </div>
                    <!-- End of form group -->

                    <!-- form group -->
                    <div class="form-group">
                      <input
                        type="email"
                        name="email"
                        class="theme-input-style"
                        placeholder="Email"
                        required
                      />
                    </div>
                    <!--End of form group -->

                    <!-- form group -->
                    <div class="form-group">
                      <input
                        type="text"
                        name="subject"
                        class="theme-input-style"
                        placeholder="subject"
                      />
                    </div>
                    <!--ENd of form group -->

                    <!-- form group -->
                    <div class="form-group">
                      <textarea
                        name="message"
                        placeholder="Write Your Message"
                        class="theme-input-style"
                        required=""
                      ></textarea>
                    </div>
                    <!--End of form group -->
                    <button
                      type="submit"
                      class="btn contact-btn btn-transparent style-4"
                    >
                      Send
                    </button>
                  </form>
                  <div class="form-response"></div>
                </div>
                <!--End of contact form -->
              </div>
            </div>
          </div>
          <div class="col-md-6">
            <!-- google map -->
            <div class="google-map">
              <div id="google-map"></div>
            </div>
            <!-- End of google map -->
          </div>
