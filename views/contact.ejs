    <!-- contact section -->
    <section class="pt-130 pb-130" data-animate="fadeInUp" data-delay="1.05">
      <div class="container">
        <div class="row align-items-center">
          <div class="col-md-6">
            <!-- contact info -->
            <div class="contact-form-info">
              <h2 class="about-page-title">Terhubunglah Bersama Kami</h2>
              <p>
                Kami ingin tahu lebih banyak tentang kamu. Tim kami akan sangat
                senang untuk menjawab semua pertanyaan kamu. Lengkapi form yang
                ada dibawah dan kami akan segera menjawab pertanyaan kamu.
              </p>
            </div>
            <!-- End of contact info -->
            <div class="row">
              <div class="col-md-10">
                <!-- contact form -->
                <div class="contact-form contact-page-form parsley-validate">
                  <form action="" method="post" novalidate>
                    <!-- form group -->
                    <div class="form-group">
                      <input
                        type="text"
                        name="ubject"
                        class="theme-input-style"
                        placeholder="Subject"
                      />
                    </div>
                    <!--ENd of form group -->
                    <!-- form group -->
                    <div class="form-group">
                      <input
                        type="text"
                        name="name"
                        class="theme-input-style"
                        placeholder="Nama Lengkap"
                        required
                      />
                    </div>
                    <!-- End of form group -->

                    <!-- form group -->
                    <div class="form-group">
                      <input
                        type="email"
                        name="email"
                        class="theme-input-style"
                        placeholder="Email"
                        required
                      />
                    </div>
                    <!--End of form group -->

                    <!-- form group -->
                    <div class="form-group">
                      <textarea
                        name="message"
                        placeholder="Tuliskan pesan kamu"
                        class="theme-input-style"
                        required=""
                      ></textarea>
                    </div>
                    <!--End of form group -->
                    <button
                      type="submit"
                      class="btn contact-btn btn-transparent style-4"
                    >
                      Kirim
                    </button>
                  </form>
                  <div class="form-response"></div>
                </div>
                <!--End of contact form -->
              </div>
            </div>
          </div>
          <div class="col-md-6">
            <!-- google map -->
            <div class="google-map">
              <div id="google-map"></div>
            </div>
            <!-- End of google map -->
          </div>
        </div>
      </div>
    </section>
