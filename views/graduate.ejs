  <body>
      <% include ./partials/navbar %>

      <!-- Page title -->
      <section
      class="page-title-inner banner overlay6 home2-vs1-bg"
      data-bg-img="https://www.digitalcrafts.com/img/houston-coding-boot-camp.jpg"
   >
         <div class="page-title-wrap crypto-patern">
            <div class="container">
            <div class="row align-items-center justify-content-center">
               <div class="col">
                  <!-- page title inner -->
                  <div
                  class="page-title text-center"
                  data-animate="fadeInUp"
                  data-delay="1.05"
                  >
                     <h1 class="about-page-banner">Profile lulusan kami</h1>
                     <ul class="nav m-0 justify-content-center">
                        <li><a href="/" title="Home">Home</a></li>
